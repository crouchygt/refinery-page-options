(
    function($) {
        $(document).ready(function () {
            $('.add_options').on('click', function (e) {
                var time = new Date().getTime(),
                    regex = new RegExp($(this).data('id'), 'g');

                $(this).before($(this).data('fields').replace(regex, time));
                e.preventDefault();
            });
        });
    }
)(jQuery);